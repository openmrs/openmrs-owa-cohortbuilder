    },
    "dependencies": {
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-react": "^6.9.0",
        "mocha": "^3.2.0",
        "react": "^15.4.1",
        "react-dom": "^15.4.1",
        "react-redux": "^5.0.1",
        "react-router": "^3.0.0",
        "redux": "^3.6.0",
        "redux-promise-middleware": "^4.2.0",
        "redux-thunk": "^2.1.0",
        "sinon": "^1.17.7"
    },
    "devDependencies": {
        "archiver": "^1.0.0",
        "babel-core": "^6.2.1",
        "babel-loader": "^6.2.0",
        "babel-preset-es2015": "^6.22.0",
        "babel-preset-react": "^6.22.0",
        "babel-preset-stage-0": "^6.22.0",
        "babel-register": "^6.22.0",
        "browser-sync": "^2.11.1",
        "browser-sync-webpack-plugin": "^1.0.1",
        "chai": "^3.5.0",
        "copy-webpack-plugin": "^4.0.1",
        "css-loader": "^0.23.1",
        "enzyme": "^2.7.1",
        "eslint": "^3.14.1",
        "extract-text-webpack-plugin": "^1.0.1",
        "file-loader": "^0.8.5",
        "html-loader": "^0.4.3",
