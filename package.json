{
  "name": "cohortbuilder",
  "version": "0.1.0",
  "description": "modern refresh of the OpenMRS Cohort Builder tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/openmrs/openmrs-owa-cohortbuilder"
  },
  "dependencies": {
    "bower": "^1.8.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "file-saver": "^1.3.3",
    "mocha": "^3.2.0",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.10",
    "react-bootstrap-date-picker": "^5.0.1",
    "react-bootstrap-typeahead": "1.2.0",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-select": "^1.0.0-rc.4",
    "redux": "^3.6.0",
    "redux-promise-middleware": "^4.2.0",
    "redux-thunk": "^2.1.0",
    "shortid": "^2.2.8",
    "sinon": "^1.17.7",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "archiver": "^1.0.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "browser-sync": "^2.11.1",
    "browser-sync-webpack-plugin": "^1.0.1",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.13.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.14.1",
    "eslint-watch": "^3.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.24.1",
    "http-response-object": "^1.1.0",
    "istanbul": "^0.4.5",
    "jsdom": "^9.10.0",
    "on-build-webpack": "^0.1.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "yargs": "^4.3.1"
  },
  "scripts": {
    "postinstall": "./node_modules/.bin/bower install",
    "clean": "rimraf dist && rimraf coverage*",
    "build": "npm run clean && webpack --progress --colors --mode=production --target=web",
    "build:dev": "npm run clean && webpack --progress --colors --mode=dev --target=web",
    "build:prod": "npm run test && npm run build",
    "build:deploy": "webpack --progress --colors --mode=deploy --target=web",
    "build:travis": "npm run build:prod",
    "watch": "webpack --progress --colors --watch --mode=deploy --target=web",
    "test": "node_modules/.bin/mocha tests/*.test.* tests/**/*.test.* --require tests/setUp.js",
    "coverage": "rimraf coverage && istanbul cover _mocha tests/*.test.* tests/**/*.test.* -- --require tests/setUp.js -R spec",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "node_modules/.bin/esw webpack.config.* app",
    "lint:watch": "npm run lint -- --watch"
  },
  "keywords": [
    "OpenMRS",
    "Open",
    "Web",
    "App"
  ],
  "author": "andela-odaniel",
  "license": "MPL-2.0"
}
